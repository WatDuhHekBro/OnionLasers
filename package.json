{
    "name": "onion-lasers",
    "version": "1.0.0",
    "description": "A command framework for Discord bots with an emphasis on making full use of command arguments.",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "test": "mocha --require ts-node/register --extension ts --recursive --unhandled-rejections=strict",
        "format": "prettier --write **/*",
        "prepare": "husky install",
        "prepack": "npm run build"
    },
    "dependencies": {
        "discord.js": "^12.5.3",
        "glob": "^7.1.6"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.3",
        "@types/node": "^14.6.4",
        "@types/ws": "^7.2.6",
        "husky": "^6.0.0",
        "mocha": "^8.1.3",
        "prettier": "^2.2.1",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    },
    "types": "typings/index.d.ts",
    "author": "WatDuhHekBro",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WatDuhHekBro/OnionLasers.git"
    },
    "bugs": {
        "url": "https://github.com/WatDuhHekBro/OnionLasers/issues"
    },
    "homepage": "https://github.com/WatDuhHekBro/OnionLasers#readme",
    "keywords": [
        "discord",
        "discord.js",
        "bot",
        "command",
        "handler"
    ]
}
