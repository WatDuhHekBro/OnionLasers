{
    "name": "onion-lasers",
    "version": "1.1.3",
    "description": "A declarative, structure-agnostic, TypeScript-centric command handler for discord.js.",
    "main": "dist/index.js",
    "scripts": {
        "start": "tsc-watch --onSuccess \"npm run build\"",
        "build": "rimraf dist typings && tsc",
        "test": "mocha --require ts-node/register --extension ts --recursive --unhandled-rejections=strict",
        "format": "prettier --write **/*",
        "prepare": "husky install",
        "prepack": "npm run build"
    },
    "dependencies": {
        "discord.js": "^13.3.1",
        "glob": "^7.2.0"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.12",
        "@types/ws": "^8.2.2",
        "dotenv": "^10.0.0",
        "husky": "^7.0.4",
        "mocha": "^9.1.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.4.0",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.5.2"
    },
    "types": "typings/index.d.ts",
    "author": "WatDuhHekBro",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WatDuhHekBro/OnionLasers.git"
    },
    "bugs": {
        "url": "https://github.com/WatDuhHekBro/OnionLasers/issues"
    },
    "homepage": "https://github.com/WatDuhHekBro/OnionLasers#readme",
    "keywords": [
        "discord",
        "discord.js",
        "discord bot",
        "command handler",
        "typescript"
    ]
}
