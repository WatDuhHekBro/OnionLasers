{
    "name": "onion-lasers",
    "version": "1.1.2",
    "description": "A declarative, structure-agnostic, TypeScript-centric command handler for discord.js.",
    "main": "dist/index.js",
    "scripts": {
        "start": "tsc-watch --onSuccess \"npm run build\"",
        "build": "gulp && tsc",
        "test": "mocha --require ts-node/register --extension ts --recursive --unhandled-rejections=strict",
        "format": "prettier --write **/*",
        "prepare": "husky install",
        "prepack": "npm run build"
    },
    "dependencies": {
        "discord.js": "^12.5.3",
        "glob": "^7.1.6"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "@types/ws": "^7.4.1",
        "del": "^6.0.0",
        "dotenv": "^8.2.0",
        "gulp": "^4.0.2",
        "husky": "^6.0.0",
        "mocha": "^8.3.2",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.2.4"
    },
    "types": "typings/index.d.ts",
    "author": "WatDuhHekBro",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WatDuhHekBro/OnionLasers.git"
    },
    "bugs": {
        "url": "https://github.com/WatDuhHekBro/OnionLasers/issues"
    },
    "homepage": "https://github.com/WatDuhHekBro/OnionLasers#readme",
    "keywords": [
        "discord",
        "discord.js",
        "discord bot",
        "command handler",
        "typescript"
    ]
}
